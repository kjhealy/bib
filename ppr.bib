	journal = {Philosophy and Phenomenological Research},
	year = {2000},
	volume = {61(2)},
	pages = {415-430},
	keywords = {duplication, epistemology, identity, intrinsic, knowledge},
	abstract = {The obvious intrinsic approach--perhaps taken for granted--appeals
